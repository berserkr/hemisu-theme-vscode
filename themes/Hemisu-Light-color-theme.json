{
  "name": "Hemisu Light",
  "type": "light",
  "colors": {
    "editor.background": "#efefef",
    "editor.foreground": "#444444",
    "editorCursor.foreground": "#444444",
    "editor.selectionBackground": "#d6d6d6",
    "editorLineNumber.foreground": "#999999",
    "statusBar.background": "#dddddd",
    "activityBar.background": "#e0e0e0",
    "sideBar.background": "#f5f5f5"
  },
  "tokenColors": [
    { "scope": "comment", "settings": { "foreground": "#999999" } },
    { "scope": "string", "settings": { "foreground": "#739200" } },
    { "scope": "constant.numeric", "settings": { "foreground": "#ff0055" } },
    { "scope": "keyword", "settings": { "foreground": "#0066ff" } },
    { "scope": "entity.name.function", "settings": { "foreground": "#ff6600" } },
    { "scope": "variable", "settings": { "foreground": "#444444" } },
    { "scope": "storage.type", "settings": { "foreground": "#75507b" } }
  ]
}
